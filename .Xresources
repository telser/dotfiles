URxvt.color8:  #7F7F7F
!red
URxvt.color1:  #CD0000
URxvt.color9:  #FF0000
!green
URxvt.color2:  #00CD00
URxvt.color10: #00FF00
!yellow
URxvt.color3:  #CDCD00
URxvt.color11: #FFFF00
!blue
URxvt.color4:  #0000EE
URxvt.color12: #5C5CFF
!magenta
URxvt.color5:  #CD00CD
URxvt.color13: #FF00FF
!cyan
URxvt.color6:  #00CDCD
URxvt.color14: #00FFFF
!white
URxvt.color7:  #E5E5E5
URxvt.color15: #FFFFFF
